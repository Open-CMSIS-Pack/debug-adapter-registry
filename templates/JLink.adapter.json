{
    "data": {
        "cwd": "${workspaceFolder}/<%= solution_folder %>",
        "shell": {
            "win32": {
                "executable": "cmd.exe",
                "args": [ "/d", "/c" ]
            },
            "linux": {
                "executable": "/bin/bash",
                "args": [ "-c" ]
            },
            "darwin": {
                "executable": "/bin/bash",
                "args": [ "-c" ]
            }
        },
        "program": "<%= symbol_files[0]?.file ?? '' %>",
        "programs": "<%= symbol_files.flatMap(e => (e.pname || !processors?.length) ? e : processors.map(p => ({ ...e, pname: p }))).groupedBy(v => v.pname ?? '') %>",
        "JLinkGDBServerCL": "JLinkGDBServerCL<%= platform === 'win32' ? '.exe' : '' %>",
        "JLink": "JLink<%= platform === 'win32' ? '.exe' : 'Exe' %>",
        "jlinkDevice": "<%= device_name %><%= start_pname ? `_${start_pname}` : '' %>",
        "jlinkDevices": "<%= Object.fromEntries(processors.map(pname => [pname ?? '', `${device_name}_${pname ?? start_pname ?? ''}`])) %>",
        "defaultDebugClock": 4000000,
        "debugClockScaler": 1000,
        "defaultResetType": 0,
        "resetTypes": {
            "Normal": 0,
            "Core": 1,
            "Reset Pin": 2
        },
        // TODO: null not working as assumed
        "defaultTerminalPortOption": null,
        "terminalPortOptions": {
            "Disabled": null,
            "Semihosting": "TelnetPort",
            "SWO": "TelnetPort",
            "RTT": "RTTTelnetPort"
        },
        "defaultTerminalPort": "4444",
        "defaultSWOPort": "5555",
        "defaultTraceClock": 12000
    },
    "launch": {
        "singlecore-launch": {
            "name": "JLink (launch)",
            "type": "gdbtarget",
            "request": "launch",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.program %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "preLaunchTask": "CMSIS Load",
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>",
                "monitor reset <%= data.resetTypes[config.reset] ?? data.defaultResetType %>\n",
                "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                "monitor SWO EnableTarget 0 <%= config.trace?.[\"trace-clock\"] ?? data.defaultTraceClock %> 1 0",
                "<% } %>",
                "tbreak main"
            ],
            "customResetCommands": [
                "monitor reset <%= data.resetTypes[config.reset] ?? data.defaultResetType %>\n",
                "tbreak main",
                "continue"
            ],
            "target": {
                "server": "<%= data.JLinkGDBServerCL %>",
                "serverParameters": [
                    "-device", "<%= device_name %>\n",
                    "-endian", "little",
                    "-if", "<%= config.protocol ?? 'swd' %>\n",
                    "-speed", "<%= parseInt((config.clock ?? data.defaultDebugClock) / data.debugClockScaler) %>\n",
                    "-noir",
                    "-vd",
                    "-nogui",
                    "-localhostonly",
                    "-port", "<%= ports.get(pname) %>\n",
                    "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                    "-SWOPort",
                    "<%= config.trace?.[\"trace-port\"] ?? data.defaultSWOPort %>\n",
                    "<% } %>",
                    "<% if (data.terminalPortOptions[config.terminal?.mode]) { %>",
                    "-<%= data.terminalPortOptions[config.terminal?.mode] %>\n",
                    "<%= config.terminal?.port ?? data.defaultTerminalPort %>\n",
                    "<% } %>"
                ],
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        },
        "singlecore-attach": {
            "name": "JLink (attach)",
            "type": "gdbtarget",
            "request": "attach",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.program %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>",
                "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                "monitor SWO EnableTarget 0 <%= config.trace?.[\"trace-clock\"] ?? data.defaultTraceClock %> 1 0",
                "<% } %>"
            ],
            "customResetCommands": [
                "monitor reset <%= data.resetTypes[config.reset] ?? data.defaultResetType %>\n",
                "tbreak main",
                "continue"
            ],
            "target": {
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        },
        "multicore-start-launch": {
            "name": "<%= pname %> JLink (launch)",
            "type": "gdbtarget",
            "request": "launch",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.programs[pname]?.[0].file ?? '' %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "preLaunchTask": "CMSIS Load",
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>",
                "monitor reset <%= data.resetTypes[config.reset] ?? data.defaultResetType %>\n",
                "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                "monitor SWO EnableTarget 0 <%= config.trace?.[\"trace-clock\"] ?? data.defaultTraceClock %> 1 0",
                "<% } %>",
                "tbreak main"
            ],
            "customResetCommands": [
                "monitor reset <%= data.resetTypes[config.reset] ?? data.defaultResetType %>\n",
                "tbreak main",
                "continue"
            ],
            "target": {
                "server": "<%= data.JLinkGDBServerCL %>",
                "serverParameters": [
                    "-device", "<%= data.jlinkDevices[pname] %>\n",
                    "-endian", "little",
                    "-if", "<%= config.protocol ?? 'swd' %>\n",
                    "-speed", "<%= parseInt((config.clock ?? data.defaultDebugClock) / data.debugClockScaler) %>\n",
                    "-noir",
                    "-vd",
                    "-nogui",
                    "-localhostonly",
                    "-port", "<%= ports.get(pname) %>\n",
                    "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                    "-SWOPort",
                    "<%= config.trace?.[\"trace-port\"] ?? data.defaultSWOPort %>\n",
                    "<% } %>",
                    "<% if (data.terminalPortOptions[config.terminal?.mode]) { %>",
                    "-<%= data.terminalPortOptions[config.terminal?.mode] %>\n",
                    "<%= config.terminal?.port ?? data.defaultTerminalPort %>\n",
                    "<% } %>"
                ],
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        },
        "multicore-start-attach": {
            "name": "<%= pname %> JLink (attach)",
            "type": "gdbtarget",
            "request": "attach",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.programs[pname]?.[0]?.file ?? '' %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>",
                "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                "monitor SWO EnableTarget 0 <%= config.trace?.[\"trace-clock\"] ?? data.defaultTraceClock %> 1 0",
                "<% } %>"
            ],
            "customResetCommands": [
                "monitor reset <%= data.resetTypes[config.reset] ?? data.defaultResetType %>\n",
                "tbreak main",
                "continue"
            ],
            "target": {
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        },
        "multicore-other": {
            "name": "<%= pname %> JLink (launch)",
            "type": "gdbtarget",
            "request": "launch",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.programs[pname]?.[0]?.file ?? '' %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>",
                "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                "monitor SWO EnableTarget 0 <%= config.trace?.[\"trace-clock\"] ?? data.defaultTraceClock %> 1 0",
                "<% } %>"
            ],
            "customResetCommands": [
                "monitor reset <%= data.resetTypes[config.reset] ?? data.defaultResetType %>\n",
                "tbreak main",
                "continue"
            ],
            "target": {
                "server": "<%= data.JLinkGDBServerCL %>",
                "serverParameters": [
                    "-device", "<%= data.jlinkDevices[pname] %>\n",
                    "-endian", "little",
                    "-if", "<%= config.protocol ?? 'swd' %>\n",
                    "-speed", "<%= parseInt((config.clock ?? data.defaultDebugClock) / data.debugClockScaler) %>\n",
                    "-noir",
                    "-vd",
                    "-nogui",
                    "-localhostonly",
                    "-port", "<%= ports.get(pname) %>\n",
                    "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                    "-SWOPort",
                    "<%= config.trace?.[\"trace-port\"] ?? data.defaultSWOPort %>\n",
                    "<% } %>",
                    "<% if (data.terminalPortOptions[config.terminal?.mode]) { %>",
                    "-<%= data.terminalPortOptions[config.terminal?.mode] %>\n",
                    "<%= config.terminal?.port ?? data.defaultTerminalPort %>\n",
                    "<% } %>"
                ],
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        }
    },
    "tasks": [
        {
            "label": "CMSIS Erase",
            "type": "shell",
            "command": [
                "echo Connect > .cmd.jlink &&",
                "echo Erase >> .cmd.jlink &&",
                "echo Exit >> .cmd.jlink &&",
                "<%= data.JLink %> -device <%= data.jlinkDevice %> -if <%= config.protocol ?? 'swd' %> -speed <%= parseInt((config.clock ?? data.defaultDebugClock) / data.debugClockScaler) %> -commandfile .cmd.jlink"
            ],
            "options": {
                "cwd": "<%= data.cwd %>",
                "shell": "<%= data.shell[process.platform] %>"
            },
            "problemMatcher": []
        },
        {
            "label": "CMSIS Load",
            "type": "shell",
            "command": [
                "echo Connect > .cmd.jlink &&",
                "<%= image_files.map(image => `echo LoadFile \"${image.file}\" >> .cmd.jlink &&\\n`).join('') %>",
                "echo Exit >> .cmd.jlink &&",
                "<%= data.JLink %> -device <%= data.jlinkDevice %> -if <%= config.protocol ?? 'swd' %> -speed <%= parseInt((config.clock ?? data.defaultDebugClock) / data.debugClockScaler) %> -commandfile .cmd.jlink"
            ],
            "options": {
                "cwd": "<%= data.cwd %>",
                "shell": "<%= data.shell[process.platform] %>"
            },
            "problemMatcher": []
        },
        {
            "label": "CMSIS Run",
            "type": "shell",
            "command": "<%= data.JLinkGDBServerCL %>",
            "options": {
                "cwd": "<%= data.cwd %>",
                "shell": "<%= data.shell[process.platform] %>"
            },
            "args": [
                "-device", "<%= data.jlinkDevice %>\n",
                "-endian", "little",
                "-if", "<%= config.protocol ?? 'swd' %>\n",
                "-speed", "<%= parseInt((config.clock ?? data.defaultDebugClock) / data.debugClockScaler) %>\n",
                "-nohalt",
                "-noir",
                "-nogui",
                "-localhostonly",
                "-port", "<%= ports.get(start_pname) %>\n",
                "<% if (config.trace?.[\"trace-mode\"] === 'SWO-UART') { %>",
                "-SWOPort", "<%= config.trace?.[\"trace-port\"] ?? data.defaultSWOPort %>\n",
                "<% } %>",
                "<% if (data.terminalPortOptions[config.terminal?.mode]) { %>",
                "-<%= data.terminalPortOptions[config.terminal?.mode] %>\n",
                "<%= config.terminal?.port ?? data.defaultTerminalPort %>\n",
                "<% } %>"
            ],
            "problemMatcher": []
        },
        {
            "label": "CMSIS Load+Run",
            "dependsOn": [
                "CMSIS Load",
                "CMSIS Run"
            ],
            "dependsOrder": "sequence",
            "problemMatcher": []
        }
    ]
}
