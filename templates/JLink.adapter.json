{
    "data": {
        "cwd": "${workspaceFolder}/<%= solution_folder %>",
        "shell": {
            "win32": {
                "executable": "cmd.exe",
                "args": [ "/d", "/c" ]
            },
            "linux": {
                "executable": "/bin/bash",
                "args": [ "-c" ]
            },
            "darwin": {
                "executable": "/bin/bash",
                "args": [ "-c" ]
            }
        },
        "program": "<%= symbol_files[0]?.file ?? '' %>",
        "programs": "<%= symbol_files.flatMap(e => (e.pname || !processors?.length) ? e : processors.map(p => ({ ...e, pname: p }))).groupedBy(v => v.pname ?? '') %>",
        "JLinkGDBServerCL": "JLinkGDBServerCL<%= platform === 'win32' ? '.exe' : '' %>",
        "JLink": "JLink<%= platform === 'win32' ? '.exe' : 'Exe' %>",
        "jlinkDevice": "<%= device_name %><%= start_pname ? `_${start_pname}` : '' %>",
        "jlinkDevices": "<%= Object.fromEntries(processors.map(pname => [pname ?? '', `${device_name}_${pname ?? start_pname ?? ''}`])) %>",
        "defaultClock": 4000,
        "scaleClock": 1000
    },
    "launch": {
        "singlecore-launch": {
            "name": "JLink (launch)",
            "type": "gdbtarget",
            "request": "launch",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.program %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "preLaunchTask": "CMSIS Load",
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>",
                "monitor reset",
                "tbreak main"
            ],
            "customResetCommands": [
                "monitor reset",
                "maintenance flush register-cache",
                "maintenance flush dcache",
                "tbreak main",
                "continue"
            ],
            "target": {
                "server": "<%= data.JLinkGDBServerCL %>",
                "serverParameters": [
                    "-device", "<%= device_name %>\n",
                    "-endian", "little",
                    "-if", "<%= config.protocol ?? 'swd' %>\n",
                    "-speed", "<%= parseInt((config.clock ?? data.defaultClock) / data.scaleClock) %>\n",
                    "-noir",
                    "-vd",
                    "-nogui",
                    "-localhostonly",
                    "-port", "<%= ports.get(pname) %>\n"
                ],
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        },
        "singlecore-attach": {
            "name": "JLink (attach)",
            "type": "gdbtarget",
            "request": "attach",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.program %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>"
            ],
            "customResetCommands": [
                "monitor reset",
                "maintenance flush register-cache",
                "maintenance flush dcache",
                "tbreak main",
                "continue"
            ],
            "target": {
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        },
        "multicore-start-launch": {
            "name": "<%= pname %> JLink (launch)",
            "type": "gdbtarget",
            "request": "launch",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.programs[pname]?.[0].file ?? '' %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "preLaunchTask": "CMSIS Load",
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>",
                "monitor reset",
                "tbreak main"
            ],
            "customResetCommands": [
                "monitor reset",
                "maintenance flush register-cache",
                "maintenance flush dcache",
                "tbreak main",
                "continue"
            ],
            "target": {
                "server": "<%= data.JLinkGDBServerCL %>",
                "serverParameters": [
                    "-device", "<%= data.jlinkDevices[pname] %>\n",
                    "-endian", "little",
                    "-if", "<%= config.protocol ?? 'swd' %>\n",
                    "-speed", "<%= parseInt((config.clock ?? data.defaultClock) / data.scaleClock) %>\n",
                    "-noir",
                    "-vd",
                    "-nogui",
                    "-localhostonly",
                    "-port", "<%= ports.get(pname) %>\n"
                ],
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        },
        "multicore-start-attach": {
            "name": "<%= pname %> JLink (attach)",
            "type": "gdbtarget",
            "request": "attach",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.programs[pname]?.[0]?.file ?? '' %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>"
            ],
            "customResetCommands": [
                "monitor reset",
                "maintenance flush register-cache",
                "maintenance flush dcache",
                "tbreak main",
                "continue"
            ],
            "target": {
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        },
        "multicore-other": {
            "name": "<%= pname %> JLink (launch)",
            "type": "gdbtarget",
            "request": "launch",
            "cwd": "<%= data.cwd %>",
            "program": "<%= data.programs[pname]?.[0]?.file ?? '' %>",
            "gdb": "arm-none-eabi-gdb",
            "auxiliaryGdb": true,
            "initCommands": [
                "<%= data.programs[pname]?.slice(1)?.map(p => p.file ? `symbol-file \"${p.file}\"\\n` : '')?.join('') %>"
            ],
            "customResetCommands": [
                "monitor reset",
                "maintenance flush register-cache",
                "maintenance flush dcache",
                "tbreak main",
                "continue"
            ],
            "target": {
                "server": "<%= data.JLinkGDBServerCL %>",
                "serverParameters": [
                    "-device", "<%= data.jlinkDevices[pname] %>\n",
                    "-endian", "little",
                    "-if", "<%= config.protocol ?? 'swd' %>\n",
                    "-speed", "<%= parseInt((config.clock ?? data.defaultClock) / data.scaleClock) %>\n",
                    "-noir",
                    "-vd",
                    "-nogui",
                    "-localhostonly",
                    "-port", "<%= ports.get(pname) %>\n"
                ],
                "port": "<%= ports.get(pname) %>"
            },
            "cmsis": {
                "cbuildRunFile": "${command:cmsis-csolution.getCbuildRunFile}",
                "updateConfiguration": "auto"
            }
        }
    },
    "tasks": [
        {
            "label": "CMSIS Erase",
            "type": "shell",
            "command": [
                "echo Connect > .cmd.jlink &&",
                "echo Erase >> .cmd.jlink &&",
                "echo Exit >> .cmd.jlink &&",
                "<%= data.JLink %> -device <%= data.jlinkDevice %> -if <%= config.protocol ?? 'swd' %> -speed <%= parseInt((config.clock ?? data.defaultClock) / data.scaleClock) %> -commandfile .cmd.jlink"
            ],
            "options": {
                "cwd": "<%= data.cwd %>",
                "shell": "<%= data.shell[process.platform] %>"
            },
            "problemMatcher": []
        },
        {
            "label": "CMSIS Load",
            "type": "shell",
            "command": [
                "echo Connect > .cmd.jlink &&",
                "<%= image_files.map(image => `echo LoadFile \"${image.file}\" >> .cmd.jlink &&\\n`).join('') %>",
                "echo Exit >> .cmd.jlink &&",
                "<%= data.JLink %> -device <%= data.jlinkDevice %> -if <%= config.protocol ?? 'swd' %> -speed <%= parseInt((config.clock ?? data.defaultClock) / data.scaleClock) %> -commandfile .cmd.jlink"
            ],
            "options": {
                "cwd": "<%= data.cwd %>",
                "shell": "<%= data.shell[process.platform] %>"
            },
            "problemMatcher": []
        },
        {
            "label": "CMSIS Run",
            "type": "shell",
            "command": "<%= data.JLinkGDBServerCL %>",
            "options": {
                "cwd": "<%= data.cwd %>",
                "shell": "<%= data.shell[process.platform] %>"
            },
            "args": [
                "-device", "<%= data.jlinkDevice %>\n",
                "-endian", "little",
                "-if", "<%= config.protocol ?? 'swd' %>",
                "-speed", "<%= parseInt((config.clock ?? data.defaultClock) / data.scaleClock) %>\n",
                "-nohalt",
                "-noir",
                "-nogui",
                "-localhostonly",
                "-port", "<%= ports.get(start_pname) %>\n"
            ],
            "problemMatcher": []
        },
        {
            "label": "CMSIS Load+Run",
            "dependsOn": [
                "CMSIS Load",
                "CMSIS Run"
            ],
            "dependsOrder": "sequence",
            "problemMatcher": []
        }
    ]
}
